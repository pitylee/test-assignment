{
    "private": true,
    "scripts": {
        "set-env": "export $(cat .env | grep \"^[^#;]\" |xargs)",
        "dev": "npm run set-env && npm run development",
        "development": "mix",
        "watch": "npm run set-env && npx mix watch",
        "watch-poll": "npm run set-env && npx mix watch -- --watch-options-poll=1000",
        "hot": "npm run set-env && npx mix watch --hot -- --host 0.0.0.0 --port 8080 --watch-options-poll=1000",
        "prod": "npm run set-env && npm run production",
        "production": "mix --production"
    },
    "devDependencies": {
        "@popperjs/core": "^2.10.2",
        "@vue/compiler-sfc": "^3.4.15",
        "autoprefixer": "^10.4.4",
        "axios": "^1.6.7",
        "bootstrap": "^5.1.3",
        "browser-sync": "^3.0.2",
        "browser-sync-webpack-plugin": "^2.3.0",
        "cross-env": "^7.0.3",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.19",
        "postcss": "^8.4.12",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.32.11",
        "sass-loader": "^11.0.1",
        "tailwindcss": "~3.0.23",
        "vue": "~2.6.12",
        "vue-loader": "^15.11.1",
        "vue-template-compiler": "~2.6.12"
    },
    "dependencies": {
        "mix-env-file": "^0.1.1"
    }
}
